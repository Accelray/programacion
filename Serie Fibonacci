#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include <stdlib.h>
using namespace std;

int main()
{
	int n,x=0,z=1,y=1,i=0;
	cout<<"Ingresar numero: ";
	cin>>n;
	cout<<"1 ";
	for(int i=1;i<=n;i++)
	{
		z=x+y;
		cout<<z<<" ";
		x=y;
		y=z;
	}
	cout<<endl;
	system("pause");
	return 0;
}
